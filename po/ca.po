#: data/sound-juicer.schemas.in.h:2
#, no-c-format
msgid ""
"%at -- album title %aT -- album title (lowercase) %aa -- album artist %aA -- "
"album artist (lowercase) %tt -- track title %tT -- track title (lowercase) %"
"ta -- track artist %tA -- track artist (lowercase)"
msgstr "%at -- títol de l'àlbum %aT -- títol de l'àlbum (en minúscules) %aa -- artista de l'àlbum %aA -- artista de l'àlbum (en minúscules) %tt -- títol de la pista %tT -- títol de la pista (en minúscules) %ta -- artista de la pista %tA --artista de la pista (en minúscules)"

#: data/sound-juicer.schemas.in.h:4
#, no-c-format
msgid ""
"Do not specify an extension. %at -- album title %aT -- album title "
"(lowercase) %aa -- album artist %aA -- album artist (lowercase) %tn -- track "
"number (i.e 8) %tN -- track number, zero padded (i.e 08) %tt -- track title %"
"tT -- track title (lowercase) %ta -- track artist %tA -- track artist "
"(lowercase)"
msgstr "No especifica una extensió. %at -- títol de l'àlbum %aT -- títol de l'àlbum (en minúscules) %aa -- artista de l'àlbum %aA -- artista de l'àlbum %tn -- número de pista (p.e 8) %tN -- número de pista, plenat amb zeros (p.e 08) %tt -- títol de la pista %tT -- títol de la pista (en minúscules) %ta -- artista de la pista %tA -- artista de la pista (en minúscules)"

#: data/sound-juicer.schemas.in.h:5
msgid ""
"If enabled, special characters such as space, wildcards and backslashes will "
"be removed from the output filename."
msgstr ""
"Si s'habilita, s'eliminaran els caràcters especials, com espais, comodins i "
"barres invertides, del nom de fitxer d'eixida."

#: data/sound-juicer.schemas.in.h:6
msgid "If to strip special characters from filenames"
msgstr "Si s'ha d'eliminar els caràcters especials dels noms de fitxer"

#: data/sound-juicer.schemas.in.h:7
msgid ""
"Paranoia mode, 3 settings: 0 (no paranoia), 4 (cdda2wav-style overlap "
"checking) and 255 (full paranoia)"
msgstr ""
"Mode de paranoia, 3 configuracions: 0 (sense paranoia), 4 (comprovació "
"d'encobriments) i 255 (paranoia total)"

