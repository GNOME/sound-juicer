msgstr "Zgradba ravni map za datoteke"

#: data/org.gnome.sound-juicer.gschema.xml:41
msgid ""
"Format specifiers: %at - album title; %aT - album title (lowercase); %aa - "
"album artist; %aA - album artist (lowercase); %as - album artist (sortable); "
"%aS - album artist (sortable lowercase); %ac - album composer; %aC - album "
"composer (lowercase); %ap - album composer (sortable); %aP - album composer "
"(sortable lowercase); %ay - album year; %tt - track title; %tT - track title "
"(lowercase); %ta - track artist; %tA - track artist (lowercase); %ts - track "
"artist (sortable); %tS - track artist (sortable lowercase); %tc - track "
"composer; %tC - track composer (lowercase); %tp - track composer (sortable); "
"%tP - track composer (sortable lowercase)."
msgstr ""
"Specifikatorji zapisa: %at - naslov albuma; %aT - naslov albuma (male črke); "
"%aa - izvajalec albuma; %aA - izvajalec albuma (male črke); %as - izvajalec "
"albuma (razvrstljivo); %aS - izvajalec albuma (razvrstljivo, male črke); %ac "
"- skladatelj albuma; %aC - skladatelj albuma (male črke); %ap - skladatelj "
"albuma (razvrstljivo); %aP - skladatelj albuma (razvrstljivo, male črke); "
"%ay - leto izida albuma; %tt - naslov skladbe; %tT - naslov skladbe (male "
"črke); %ta - izvajalec skladbe; %tA - izvajalec skladbe (male črke); %ts - "
"izvajalec skladbe (razvrstljivo); %tS - izvajalec skladbe (razvrstljivo, "
"male črke); %tc - skladatelj skladbe; %tC - skladatelj skladbe (male črke); "
"%tp - skladatelj skladbe (razvrstljivo); %tP - skladatelj skladbe "
"(razvrstljivo, male črke)."

#: data/org.gnome.sound-juicer.gschema.xml:46
msgid "The name pattern for files"
msgstr "Vzorec poimenovanja datotek"

#: data/org.gnome.sound-juicer.gschema.xml:47
msgid ""
"Do not specify an extension. Format specifiers: %at - album title; %aT - "
"album title (lowercase); %aa - album artist; %aA - album artist (lowercase); "
"%as - album artist (sortable); %aS - album artist (sortable lowercase); %ac "
"- album composer; %aC - album composer (lowercase); %ap - album composer "
"(sortable); %aP - album composer (sortable lowercase); %tn - track number (i."
"e 8); %tN - track number, zero padded (i.e 08); %tt - track title; %tT - "
"track title (lowercase); %ta - track artist; %tA - track artist (lowercase); "
"%ts - track artist (sortable); %tS - track artist (sortable lowercase); %tc "
"- track composer; %tC - track composer (lowercase); %tp - track composer "
"(sortable); %tP - track composer (sortable lowercase); %dn - disc and track "
"number (i.e Disk 2 - 6, or 6); %dN - disc number, zero padded (i.e d02t06, "
"or 06)."
msgstr ""
"Ne navedite končnice. Specifikatorji zapisa: %at - naslov albuma; %aT - "
"naslov albuma (male črke); %aa - izvajalec albuma; %aA - izvajalec albuma "
"(male črke); %as - izvajalec albuma (razvrstljivo); %aS - izvajalec albuma "
"(razvrstljivo, male črke); %ac - skladatelj albuma; %aC - skladatelj albuma "
"(male črke); %ap - skladatelj albuma (razvrstljivo); %aP - skladatelj albuma "
"(razvrstljivo, male črke); %tn - številka sledi (tj.e 8); %tN - številka "
"skladbe, z ničelnim polnilom (i.e 08); %tt - naslov skladbe; %tT - naslov "
"skladbe (male črke); %ta - izvajalec skladbe; %tA - izvajalec skladbe (male "
"črke); %ts - izvajalec skladbe (razvrstljivo); %tS - izvajalec skladbe "
"(razvrstljivo, male črke); %tc - skladatelj skladbe; %tC - skladatelj "
"skladbe (male črke); %tp - skladatelj skladbe (razvrstljivo); %tP - "
"skladatelj skladbe (razvrstljivo, male črke); %dn - številka plošče in "
"skladbe (i.npr. Disk 2 - 6 ali 6); %dN - številka diska z ničelnim polnilom "
"(npr. d02t06 ali 06)."

